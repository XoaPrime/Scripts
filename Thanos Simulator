local function tween(o, props, t, easingStyle, easingDirection)
    TweenService:Create(o, TweenInfo.new(t or .25, easingStyle or Enum.EasingStyle.Quad, easingDirection or Enum.EasingDirection.Out), props):Play()
end

local function addHoverEffect(button, normalColor, hoverColor)
    button.MouseEnter:Connect(function()
        tween(button, {BackgroundColor3 = hoverColor})
    end)
    button.MouseLeave:Connect(function()
        tween(button, {BackgroundColor3 = normalColor})
    end)
end

local OFF_COLOR = Color3.fromRGB(180,80,90)
local ON_COLOR = Color3.fromRGB(80,180,100)
local OFF_HOVER_COLOR = Color3.fromRGB(200,100,110)
local ON_HOVER_COLOR = Color3.fromRGB(100,200,120)


addHoverEffect(close, Color3.fromRGB(255,90,90), Color3.fromRGB(255,120,120))
addHoverEffect(prevEnemy, Color3.fromRGB(200,200,200), Color3.new(1,1,1))
addHoverEffect(nextEnemy, Color3.fromRGB(200,200,200), Color3.new(1,1,1))
addHoverEffect(prevMode, Color3.fromRGB(200,200,200), Color3.new(1,1,1))
addHoverEffect(nextMode, Color3.fromRGB(200,200,200), Color3.new(1,1,1))


akToggle.MouseEnter:Connect(function()

    TweenService:Cancel(akToggle)
    if looping then
        tween(akToggle, {BackgroundColor3 = ON_HOVER_COLOR}, 0.1)
    else
        tween(akToggle, {BackgroundColor3 = OFF_HOVER_COLOR}, 0.1)
    end
end)
akToggle.MouseLeave:Connect(function()

     TweenService:Cancel(akToggle)

     if looping then
        tween(akToggle, {BackgroundColor3 = ON_COLOR}, 0.1)
    else
        tween(akToggle, {BackgroundColor3 = OFF_COLOR}, 0.1)
    end
end)


freezeToggle.MouseEnter:Connect(function()
    TweenService:Cancel(freezeToggle)
    if isFrozen then
        tween(freezeToggle, {BackgroundColor3 = ON_HOVER_COLOR}, 0.1)
    else
        tween(freezeToggle, {BackgroundColor3 = OFF_HOVER_COLOR}, 0.1)
    end
end)
freezeToggle.MouseLeave:Connect(function()
     TweenService:Cancel(freezeToggle)
     if isFrozen then
        tween(freezeToggle, {BackgroundColor3 = ON_COLOR}, 0.1)
    else
        tween(freezeToggle, {BackgroundColor3 = OFF_COLOR}, 0.1)
    end
end)

local normalTeleportColor = Color3.fromRGB(80,80,120)
local hoverTeleportColor = Color3.fromRGB(100,100,150)
addHoverEffect(catButton, normalTeleportColor, hoverTeleportColor)
addHoverEffect(shopButton, normalTeleportColor, hoverTeleportColor)
addHoverEffect(centerButton, normalTeleportColor, hoverTeleportColor)
addHoverEffect(forgeButton, normalTeleportColor, hoverTeleportColor)
addHoverEffect(world5Button, normalTeleportColor, hoverTeleportColor)
addHoverEffect(lunarButton, normalTeleportColor, hoverTeleportColor)
addHoverEffect(spawnButton, normalTeleportColor, hoverTeleportColor)



prevEnemy.MouseButton1Click:Connect(function()
    selEnemyIdx = ((selEnemyIdx-2)%#ENEMY_LIST)+1
    selText.Text = ENEMY_LIST[selEnemyIdx]
    currentRootPart, currentHumanoid = nil, nil
end)
nextEnemy.MouseButton1Click:Connect(function()
    selEnemyIdx = (selEnemyIdx%#ENEMY_LIST)+1
    selText.Text = ENEMY_LIST[selEnemyIdx]
    currentRootPart, currentHumanoid = nil, nil
end)
prevMode.MouseButton1Click:Connect(function()
    selModeIdx = ((selModeIdx-2)%#TELEPORT_MODES)+1
    modeText.Text = TELEPORT_MODES[selModeIdx]
end)
nextMode.MouseButton1Click:Connect(function()
    selModeIdx = (selModeIdx%#TELEPORT_MODES)+1
    modeText.Text = TELEPORT_MODES[selModeIdx]
end)
distBox.FocusLost:Connect(function()
    local v = tonumber(distBox.Text)
    if v and v > 0 then distance = v else distBox.Text = tostring(distance) end
end)



freezeToggle.MouseButton1Click:Connect(function()
    isFrozen = not isFrozen
    local char = player.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    local root = char and char:FindFirstChild("HumanoidRootPart") 

    if hum and root then 
        if isFrozen then
            freezeToggle.Text = "Freeze [ON]"
            tween(freezeToggle, {BackgroundColor3 = ON_COLOR}, 0.1)

            root.Anchored = true
 
        else
            freezeToggle.Text = "Freeze [OFF]"
            tween(freezeToggle, {BackgroundColor3 = OFF_COLOR}, 0.1)

            root.Anchored = false
            hum.WalkSpeed = DEFAULT_WALKSPEED
            hum.JumpPower = DEFAULT_JUMPPOWER
        end
    else
        isFrozen = false
        freezeToggle.Text = "Freeze [OFF]"
        tween(freezeToggle, {BackgroundColor3 = OFF_COLOR}, 0.1)
        if hum then
            hum.WalkSpeed = DEFAULT_WALKSPEED
            hum.JumpPower = DEFAULT_JUMPPOWER
        end
    end
end)



local function pickNewTarget()
    local name = ENEMY_LIST[selEnemyIdx]
    local pool = {}
    if name == "Templar" then
        for _,obj in ipairs(workspace:GetChildren()) do
            if obj.Name == "Templar" then
                local h = obj:FindFirstChildOfClass("Humanoid")
                local r = obj:FindFirstChild("HumanoidRootPart")
                if h and r and h.Health > 0 then pool[#pool+1] = {hum=h,hrp=r} end
            end
        end
    else
        local folder = workspace:FindFirstChild("Enemies")
        if folder then
            for _,npc in ipairs(folder:GetChildren()) do
                if npc.Name == name then
                    local h = npc:FindFirstChildOfClass("Humanoid")
                    local r = npc:FindFirstChild("HumanoidRootPart")
                    if h and r and h.Health > 0 then pool[#pool+1] = {hum=h,hrp=r} end
                end
            end
        end
    end
    if #pool == 0 then return nil,nil end
    local pick = pool[math.random(#pool)]
    return pick.hrp, pick.hum
end


local function computeOffset(hrp)
    local mode = TELEPORT_MODES[selModeIdx]
    if mode=="Left" then
        return -hrp.CFrame.RightVector*distance + Vector3.new(0,0.5,0)
    elseif mode=="Right" then
        return hrp.CFrame.RightVector*distance + Vector3.new(0,0.5,0)
    elseif mode=="In Front" then
        return hrp.CFrame.LookVector*distance + Vector3.new(0,0.5,0)
    else -- Behind
        return -hrp.CFrame.LookVector*distance + Vector3.new(0,0.5,0)
    end
end


local looping, currentRootPart, currentHumanoid, conn = false,nil,nil,nil
akToggle.MouseButton1Click:Connect(function()
    looping = not looping
    if looping then
        akToggle.Text="Auto Kill [ON]"
        tween(akToggle,{BackgroundColor3=ON_COLOR}, 0.1)
        conn = RunService.RenderStepped:Connect(function()
            if not looping then conn:Disconnect() return end
            local char = player.Character
            local root = char and char:FindFirstChild("HumanoidRootPart")
            if root then
                if (not currentRootPart)
                    or (not currentHumanoid)
                    or (currentHumanoid.Health and currentHumanoid.Health <= 0)
                    or (not currentRootPart.Parent)
                then
                    currentRootPart,currentHumanoid = pickNewTarget()
                end
                if currentRootPart then
                    local pos = currentRootPart.CFrame.Position + computeOffset(currentRootPart)
                    if (root.CFrame.Position - pos).Magnitude > 0.1 then
                         root.CFrame = CFrame.new(pos, currentRootPart.CFrame.Position)
                    end
                end
            end
        end)
    else
        akToggle.Text="Auto Kill [OFF]"
        tween(akToggle,{BackgroundColor3=OFF_COLOR}, 0.1)
        if conn then conn:Disconnect() end
    end
end)


